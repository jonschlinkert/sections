{
  "name": "sections",
  "description": "Manipulate sections in a markdown string. A 'section' is a block of content preceded by a valid markdown ATX heading.",
  "version": "0.1.8",
  "homepage": "https://github.com/jonschlinkert/sections",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/sections",
  "bugs": {
    "url": "https://github.com/jonschlinkert/sections/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "define-property": "^0.2.5",
    "gfm-code-blocks": "^0.3.0"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.10",
    "mocha": "^3.0.1"
  },
  "keywords": [
    "format",
    "markdown",
    "md",
    "parse",
    "sections"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "gulp-format-md",
        "markdown-utils",
        "remarkable"
      ]
    },
    "reflinks": [
      "verb"
    ]
  }
}
